<queue-entries>
  <queue>
    <name>fetchqueue</name>
    <rate>5/m</rate>
    <bucket-size>5</bucket-size>
    <max-concurrent-requests>5</max-concurrent-requests>
    <retry-parameters>
        <task-retry-limit>2</task-retry-limit>
        <min-backoff-seconds>120</min-backoff-seconds>
    </retry-parameters>
  </queue>
</queue-entries>

