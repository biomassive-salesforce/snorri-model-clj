<queue-entries>
  <queue>
    <name>fetchqueue</name>
    <rate>2/m</rate>
    <bucket-size>2</bucket-size>
    <max-concurrent-requests>5</max-concurrent-requests>
    <retry-parameters>
        <task-retry-limit>1</task-retry-limit>
        <min-backoff-seconds>60</min-backoff-seconds>
    </retry-parameters>
  </queue>
</queue-entries>

